<h1>Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.name %></td>
        <td><%= account.active %></td>
        <td><%= link_to 'Show', account %></td>
      </tr>
    <% end %>
  </tbody>
</table>
